    <refsection id="Description_MEAI_CloserWithArc">
        <title>Description</title>
        <para>
            Ideal closing switch with simple arc model
        </para>
    </refsection>
    <refsection id="Palette_MEAI_CloserWithArc">
        <title>Palette</title>
        <itemizedlist>
            <listitem>
