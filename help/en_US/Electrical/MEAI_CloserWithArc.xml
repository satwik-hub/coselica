    <refsection>
        <title>Block Screenshot</title>
        <para>
            <inlinemediaobject>
                <imageobject>
                    <imagedata fileref="../../../images/gui/MEAI_CloserWithArc_gui.gif" align="center" valign="middle"/>
                </imageobject>
            </inlinemediaobject>
        </para>
        <itemizedlist>
            <listitem>
                <para>
                    <emphasis role="bold">Ron [Ohm] : Closed switch resistance</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 1. </para>
                <para> Default value : 0.00001</para>
            </listitem>
            <listitem>
                <para>
                    <emphasis role="bold">Goff [S] : Opened switch conductance</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 1. </para>
                <para> Default value : 0.00001</para>
            </listitem>
            <listitem>
                <para>
                    <emphasis role="bold">V0 [V] : Initial arc voltage</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 1. </para>
                <para> Default value : 30</para>
            </listitem>
            <listitem>
                <para>
                    <emphasis role="bold">dV/dt [V/s] : Arc voltage slope</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 1. </para>
                <para> Default value : 10000</para>
            </listitem>
            <listitem>
                <para>
                    <emphasis role="bold">Vmax [V] : Max. arc voltage</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 1. </para>
                <para> Default value : 60</para>
            </listitem>
        </itemizedlist>
    </refsection>
