    <refsection id="Description_CEAS_ZDiode">
        <title>Description</title>
        <para>
            Zener Diode with 3 working areas
        </para>
    </refsection>
    <refsection id="Palette_CEAS_ZDiode">
        <title>Palette</title>
        <itemizedlist>
            <listitem>
                <para>
                    <link linkend="Electrical_pal">Electrical - Coselica toolbox</link>
                </para>
            </listitem>
        </itemizedlist>
    </refsection>
    <refsection>
        <title>Block Screenshot</title>
        <para>
            <inlinemediaobject>
                <imageobject>
                    <imagedata fileref="../../../images/gui/CEAS_ZDiode_gui.gif" align="center" valign="middle"/>
                </imageobject>
            </inlinemediaobject>
        </para>
        <itemizedlist>
            <listitem>
                <para>
                    <emphasis role="bold">Ids [A] : Saturation current</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 1. </para>
                <para> Default value : 0.000001</para>
            </listitem>
            <listitem>
                <para>
                    <emphasis role="bold">Vt [V] : Voltage equivalent of temperature (kT/qn)</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 1. </para>
                <para> Default value : 0.04</para>
            </listitem>
            <listitem>
                <para>
                    <emphasis role="bold">Maxexp [-] : Max. exponent for linear continuation</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 1. </para>
                <para> Default value : 30</para>
            </listitem>
            <listitem>
                <para>
                    <emphasis role="bold">R [Ohm] : Parallel ohmic resistance</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 1. </para>
                <para> Default value : 1.000D+08</para>
            </listitem>
            <listitem>
                <para>
                    <emphasis role="bold">Bv [V] : Breakthrough voltage = Zener- or Z-voltage</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 1. </para>
                <para> Default value : 5.1</para>
            </listitem>
            <listitem>
                <para>
                    <emphasis role="bold">Ibv [A] : Breakthrough knee current</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 1. </para>
                <para> Default value : 0.7</para>
            </listitem>
            <listitem>
                <para>
                    <emphasis role="bold">Nbv [-] : Breakthrough emission coefficient</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 1. </para>
                <para> Default value : 0.74</para>
            </listitem>
        </itemizedlist>
    </refsection>
