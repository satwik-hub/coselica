    <refsection id="Description_MEAB_SaturatingInductor">
        <title>Description</title>
        <para>Simple model of an inductor with saturation</para>
    </refsection>
    <refsection id="Palette_MEAB_SaturatingInductor">
        <title>Palette</title>
        <itemizedlist>
            <listitem>
