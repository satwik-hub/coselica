    <refsection id="Description_CMPJ_Revolute">
        <title>Description</title>
        <para>Revolute joint (1 rotational degree-of-freedom, 2 states)</para>
    </refsection>
    <refsection id="Palette_CMPJ_Revolute">
        <title>Palette</title>
        <itemizedlist>
            <listitem>
                <para>
                    <link linkend="Mechanics_Planar_pal">Mechanics_Planar - Coselica toolbox</link>
                </para>
            </listitem>
        </itemizedlist>
    </refsection>
    <refsection>
        <title>Block Screenshot</title>
        <para>
            <inlinemediaobject>
                <imageobject>
                    <imagedata fileref="../../../../images/gui/CMPJ_Revolute_en_US_gui.gif" align="center" valign="middle"/>
                </imageobject>
            </inlinemediaobject>
        </para>
        <itemizedlist>
            <listitem>
                <para>
                    <emphasis role="bold">phi_offset [rad] : Relative angle offset (angle = phi + phi_offset)</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 1. </para>
                <para> Default value : 0</para>
            </listitem>
            <listitem>
                <para>
                    <emphasis role="bold">initType [-] : Type of initial value for [phi,w,z] (0=guess,1=fixed)</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 3. </para>
                <para> Default value : [0,0,0]</para>
            </listitem>
            <listitem>
                <para>
                    <emphasis role="bold">phi_start [rad] : Initial value of rotation angle phi</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 1. </para>
                <para> Default value : 0</para>
            </listitem>
            <listitem>
                <para>
                    <emphasis role="bold">w_start [rad/s] : Initial value of relative angular velocity w = der(phi)</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 1. </para>
                <para> Default value : 0</para>
            </listitem>
            <listitem>
                <para>
                    <emphasis role="bold">z_start [rad/s2] : Initial value of relative angular acceleration z = der(w)</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 1. </para>
                <para> Default value : 0</para>
            </listitem>
        </itemizedlist>
    </refsection>
