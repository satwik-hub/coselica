    <refsection>
        <title>Block Screenshot</title>
        <para>
            <inlinemediaobject>
                <imageobject>
                    <imagedata fileref="../../../images/gui/CBN_RateLimiter_gui.gif" align="center" valign="middle"/>
                </imageobject>
            </inlinemediaobject>
        </para>
        <itemizedlist>
            <listitem>
                <para>
                    <emphasis role="bold">sMax [-] : Maximum slope</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 1. </para>
                <para> Default value : 1</para>
            </listitem>
            <listitem>
                <para>
                    <emphasis role="bold">sMin [-] : Minimum slope</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 1. </para>
                <para> Default value : -1</para>
            </listitem>
            <listitem>
                <para>
                    <emphasis role="bold">T [s] : Time constant (&gt; 0)</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 1. </para>
                <para> Default value : 0.0001</para>
            </listitem>
            <listitem>
                <para>
                    <emphasis role="bold">y_start [-] : Start value of output</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 1. </para>
                <para> Default value : 0</para>
            </listitem>
        </itemizedlist>
    </refsection>
