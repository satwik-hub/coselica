    <refsection>
        <title>Block Screenshot</title>
        <para>
            <inlinemediaobject>
                <imageobject>
                    <imagedata fileref="../../../images/gui/MBC_PID_en_US_gui.gif" align="center" valign="middle"/>
                </imageobject>
            </inlinemediaobject>
        </para>
        <itemizedlist>
            <listitem>
                <para>
                    <emphasis role="bold">k [-] : Gain</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 1. </para>
                <para> Default value : 1</para>
            </listitem>
            <listitem>
                <para>
                    <emphasis role="bold">Ti [s] : Time Constant of Integrator block</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 1. </para>
                <para> Default value : 0.5</para>
            </listitem>
            <listitem>
                <para>
                    <emphasis role="bold">Td [s] : Time Constant of Derivative block</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 1. </para>
                <para> Default value : 0.1</para>
            </listitem>
            <listitem>
                <para>
                    <emphasis role="bold">Nd [-] : The higher Nd, the more ideal the Derivative block</emphasis>
                </para>
                <para> Properties : Type 'vec' of size 1. </para>
                <para> Default value : 10</para>
            </listitem>
        </itemizedlist>
    </refsection>
